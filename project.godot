; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Camera2D",
"class": "ShakeCamera2D",
"language": "GDScript",
"path": "res://src/ShakeCamera2D.gd"
} ]
_global_script_class_icons={
"ShakeCamera2D": ""
}

[application]

config/name="Case Maker"
run/main_scene="res://src/Main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=256
window/size/height=384
window/size/test_width=512
window/size/test_height=768
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[locale]

translations=PoolStringArray(  )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"

[textalog]

events/logic_event/change_timeline="res://addons/dialog_plugin/Resources/Events/LogicEvent/ChangeTimelineEvent/ChangeTiemlineEvent.gd"
events/character_event/character_change_expression="res://addons/dialog_plugin/Resources/Events/CharacterEvent/ChangeExpressionEvent/CharacterChangeExpressionEvent.gd"
events/character_event/character_join="res://addons/dialog_plugin/Resources/Events/CharacterEvent/JoinEvent/CharacterJoinEvent.gd"
events/character_event/character_leave="res://addons/dialog_plugin/Resources/Events/CharacterEvent/LeaveEvent/CharacterLeaveEvent.gd"
events/miscelaneous_event/custom="res://addons/dialog_plugin/Resources/Events/MiscelaneousEvent/CustomEvent/CustomEvent.gd"
events/logic_event/emit_signal="res://addons/dialog_plugin/Resources/Events/LogicEvent/EmitSignalEvent/EmitSignalEvent.gd"
events/logic_event/jump_to="res://addons/dialog_plugin/Resources/Events/LogicEvent/JumpToEvent/JumpToEvent.gd"
events/logic_event/question="res://addons/dialog_plugin/Resources/Events/LogicEvent/QuestionEvent/QuestionEvent.gd"
events/text_event/text="res://addons/dialog_plugin/Resources/Events/TextEvent/TextEvent.gd"
events/text_event/text_with_audio="res://addons/dialog_plugin/Resources/Events/TextEvent/TextAudioEvent/TextAudioEvent.gd"
events/logic_event/wait_time="res://addons/dialog_plugin/Resources/Events/LogicEvent/WaitTimeEvent/WaitTimeEvent.gd"
events/logic_event/condition="res://addons/dialog_plugin/Resources/Events/LogicEvent/ConditionEvent/ConditionEvent.gd"
events/miscelaneous_event/comment="res://addons/dialog_plugin/Resources/Events/MiscelaneousEvent/Comment/CommentEvent.gd"
events/logic_event/set="res://addons/dialog_plugin/Resources/Events/LogicEvent/SetEvent/SetEvent.gd"
